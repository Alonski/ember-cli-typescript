{
  "compilerOptions": {
    "target": "es2019",
    "module": "Node16",
    "moduleResolution": "node16",
    "allowJs": true,
    "noEmitOnError": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../js",
    "baseUrl": ".",
    "paths": {
      "*": [
        "./types/*"
      ]
    },
    "esModuleInterop": true,
  }
}
